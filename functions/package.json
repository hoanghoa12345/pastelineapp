{
  "name": "lambda-functions",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "cross-env NODE_PATH=./dist node  ./dist/server.js",
    "start:dev": "cross-env NODE_PATH=./src ts-node-dev --respawn ./src/server.ts",
    "dev": "nodemon src/server.ts",
    "build": "rimraf dist && tsc",
    "tsc": "tsc --noEmit",
    "package": "serverless package",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix .",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "test": "cross-env NODE_PATH=./src mocha --require ts-node/register 'src/**/*.test.ts' --timeout 20000 --exit"
  },
  "keywords": [],
  "author": "Hoang Hoa <hoanghoa@duck.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.370.0",
    "@aws-sdk/lib-dynamodb": "^3.370.0",
    "@aws-sdk/util-dynamodb": "^3.386.0",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
