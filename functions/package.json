{
  "name": "lambda-functions",
  "version": "1.0.0",
  "description": "Lambda functions for Note API",
  "scripts": {
    "start": "node ./dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "tsc": "tsc --noEmit",
    "package": "chmod +x ./build.sh && ./build.sh",
    "lint": "eslint --max-warnings 0 . && npm run tsc",
    "lint-fix": "eslint --fix src",
    "format-lint": "prettier --config .prettierrc -l --ignore-unknown .",
    "format-fix": "prettier --config .prettierrc --write --ignore-unknown .",
    "test": "mocha --require ts-node/register src/test/*.test.ts"
  },
  "keywords": [],
  "author": "Hoang Hoa <hoanghoa@duck.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.1",
    "openid-client": "^5.6.5",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.370.0",
    "@aws-sdk/lib-dynamodb": "^3.370.0",
    "@aws-sdk/util-dynamodb": "^3.386.0",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.4.8",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "chai": "^5.0.0",
    "eslint": "^8.46.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
